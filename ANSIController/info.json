{
    "BEL" :{
        "escape": "\\a",
        "Ctrl+key": "^G",
        "Hex": "\\x07",
        "Octal": "\\007",
        "Decimal": "\\7",
        "EN-Description": "Terminal bell",
        "AR-Description": "جرس"
    },
    "BS" :{
        "escape": "\\b",
        "Ctrl+key": "^H",
        "Hex": "\\x08",
        "Octal": "\\010",
        "Decimal": "\\8",
        "EN-Description": "Backspace",
        "AR-Description": "مسافة للخلف"
    },
    "HT" :{
        "escape": "\\t",
        "Ctrl+key": "^I",
        "Hex": "\\x08",
        "Octal": "\\010",
        "Decimal": "\\8",
        "EN-Description": "Horizontal TAB",
        "AR-Description": "مساحة أفقية"
    },
    "LF" :{
        "escape": "\\n",
        "Ctrl+key": "^J",
        "Hex": "\\x0A",
        "Octal": "\\012",
        "Decimal": "\\10",
        "EN-Description": "Linefeed (newline)",
        "AR-Description": "تغذية السطر (سطر جديد)"
